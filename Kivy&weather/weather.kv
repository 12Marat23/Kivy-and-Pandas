<FloatLayout>


    Image:
        id: weather_image
        source: 'image/солнце облака.png'
        allow_stretch: True
        keep_ratio: False

    Label:
        id: city_id
        font_name: "font/Alegreya-ExtraBoldItalic.ttf"
        text: ''
        pos_hint: {'center_x': 0.2, 'center_y': 0.9}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'

    Label:
        id: time_id
        font_name: "font/Alegreya-ExtraBoldItalic.ttf"
        text: ''
        pos_hint: {'center_x': 0.2, 'center_y': 0.8}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'

    Label:
        id: data_id
        font_name: "font/Alegreya-ExtraBoldItalic.ttf"
        text: ''
        pos_hint: {'center_x': 0.2, 'center_y': 0.7}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'

    Label:
        id: feels_like
        font_name: "font/Alegreya-ExtraBoldItalic.ttf"
        text: ''
        pos_hint: {'center_x': 0.5, 'center_y': 0.9}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'

    Label:
        id: humidity
        font_name: "font/Alegreya-ExtraBoldItalic.ttf"
        text: ''
        pos_hint: {'center_x': 0.5, 'center_y': 0.8}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'

    TextInput:
        id: text_id
        size_hint: 0.8, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.25}
        multiline: False

    EnterButton:
        text: 'Показать'
        size_hint: 0.4, 0.1
        pos_hint: {'center_x': 0.3, 'center_y': 0.1}
        on_release: root.press_me(text_id.text)

    Button:
        text: 'Выход'
        size_hint: 0.4, 0.1
        pos_hint: {'center_x': 0.7, 'center_y': 0.1}
        on_press: app.stop()

    Image:
        id: icon
        source: 'image/13d.png'
        size_hint: 0.3, 0.3
        pos_hint: {'center_x': 0.8, 'top': 0.9}

    Label:
        id: label_id
        font_name: "font/BELLB"
        text: ''
        pos_hint: {'center_x': 0.8, 'center_y': 0.7}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'

    Image:
        id: icon_wind
        source: 'image/wind.png'
        size_hint: 0.1, 0.1
        pos_hint: {'center_x': 0.35, 'center_y': 0.7}
    Label:
        id: wind
        font_name: "font/Alegreya-ExtraBoldItalic.ttf"
        text: ''
        pos_hint: {'center_x': 0.5, 'center_y': 0.7}
        font_size: min(root.width, root.height) * 0.05
        color: (0, 0, 0, 1)
        halign: 'center'


